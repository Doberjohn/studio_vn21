    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.90.1",
    "@vercel/analytics": "^1.6.0",
    "@vercel/speed-insights": "^1.2.0",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.3",
    "isbot": "^5.1.31",
    "lucide-react": "^0.554.0",
    "pg": "^8.16.3",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router": "^7.12.0",
    "react-router-dom": "^7.12.0",
    "reading-time-estimator": "^2.0.4",
    "tailwind-merge": "^3.4.0"
