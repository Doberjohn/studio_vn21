    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "prettier": "3.6.2",
    "prisma": "^7.2.0",
    "tailwindcss": "^4.1.13",
    "tsx": "^4.21.0",
    "typescript": "^5.9.2",
    "vite": "^7.1.7",
    "vite-tsconfig-paths": "^5.1.4"
  }
